= title t('tt.sign_in', resource_name: "CHANGE ME")
= simple_form_for(resource, :as => resource_name, :url => session_path(resource_name), :html => {:class => 'form-inline'}) do |f|
  .inputs
    = f.input :email, :required => false, :autofocus => true
    = f.input :password, :required => false
    = f.input :remember_me, :as => :boolean if devise_mapping.rememberable?
  .actions
    = f.button :submit, t('nav.signin')
= render :partial => "devise/shared/links"